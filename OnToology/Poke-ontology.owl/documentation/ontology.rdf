<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#"
     xml:base="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:poke-ontology="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology">
        <rdfs:comment>Name: Poké-onthology
Author: Pau Amich Vidal
Description: This ontology describes various pokémon based on thier types and movesets.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->


    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#comment -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#comment"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral -->


    <rdfs:Datatype rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#integer -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#integer"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#positiveInteger -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#string -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#evolvesInto -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#evolvesInto">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Pokemon"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Pokemon"/>
        <rdfs:comment>Relates a Pokémon to its evolved form.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#hasType -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#hasType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Pokemon"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#PokemonType"/>
        <rdfs:comment>Gives a type to a pokémon.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#pokemonStrongAgainst -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#pokemonStrongAgainst">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Pokemon"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#PokemonType"/>
        <rdfs:comment>Shows what types a pokémon is strong against.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#pokemonWeakAgainst -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#pokemonWeakAgainst">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Pokemon"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#PokemonType"/>
        <rdfs:comment>Shows what types a pokémon is weak against.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#strongAgainst -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#strongAgainst">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#weakAgainst"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#PokemonType"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#PokemonType"/>
        <rdfs:comment>Shows what types a type is strong against.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#weakAgainst -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#weakAgainst">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#PokemonType"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#PokemonType"/>
        <rdfs:comment>Shows what types a type is weak against.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Nickname -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Nickname">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Pokemon"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Nickname given to the Pokémon, if any.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Owner -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Owner">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Pokemon"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of the pokémon owner, if it has any.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Blastoise -->


    <owl:Class rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Blastoise">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#KnownPokemon"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#hasType"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#WaterType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Blastoise is a strong water-typed pokémon.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#BugType -->


    <owl:Class rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#BugType">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#PokemonType"/>
        <rdfs:comment>The bug type is weak against the fire and rock types, but strong against the grass type.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#BugTypePokemon -->


    <owl:Class rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#BugTypePokemon">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#Pokemon"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#hasType"/>
                        <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#BugType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Subset of Pokémon with the Bug type.</rdfs:comment>
    </owl:Class>
    


