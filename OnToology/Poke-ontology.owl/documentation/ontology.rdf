                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#GrassType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Bulbasaur is a weak grass-typed pokémon.</rdfs:comment>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://dbpedia.org/page/Bulbasaur</rdfs:sameAs>
    </owl:Class>
    


