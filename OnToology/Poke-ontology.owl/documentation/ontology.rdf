                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#hasType"/>
                        <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#ElectricType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
