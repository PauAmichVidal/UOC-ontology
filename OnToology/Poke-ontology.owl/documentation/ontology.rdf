    <!-- http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#FireType -->


    <owl:Class rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#FireType">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#PokemonType"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#strongAgainst"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#BugType"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#GrassType"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#weakAgainst"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#RockType"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/pau/ontologies/2022/poke-ontology#WaterType"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The fire type is weak against the water and rock types, but strong against the grass and bug types.</rdfs:comment>
    </owl:Class>
    


